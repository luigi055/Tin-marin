{
  "name": "tin-marin",
  "version": "1.0.0",
  "description":
    "Tin Marin app, app where you enter some options and it chose one of them randomly",
  "main": "server.js",
  "scripts": {
    "dev:ssr":
      "export NODE_ENV=server || SET \"NODE_ENV=server\" && webpack && node server.js",
    "production":
      "export NODE_ENV=production || SET \"NODE_ENV=production\" && webpack -p --optimize-minimize && node server.js",
    "dev": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "clear": "rm -rf ./public",
    "start":
      "export NODE_ENV=production || SET \"NODE_ENV=production\" && node server.js",
    "build:dev": "rm -rf ./public && webpack",
    "build:dev--watch": "webpack -d -w",
    "build":
      "export NODE_ENV=production || SET \"NODE_ENV=production\" && rm -rf ./public && webpack -p",
    "lint": "eslint **/*.{js, jsx} --quiet",
    "format":
      "prettier --write --single-quote --print-width=120 --parser=flow --tab-width=2 \"src/**/*.{js, jsx}\"",
    "test":
      "export NODE_ENV=test || SET \"NODE_ENV=test\" && ./node_modules/.bin/jest",
    "test:coverage":
      "export NODE_ENV=test || SET \"NODE_ENV=test\" && ./node_modules/.bin/jest --coverage",
    "test:watch":
      "export NODE_ENV=test || SET \"NODE_ENV=test\" && ./node_modules/.bin/jest --watch",
    "flow:init": "flow init",
    "flow:status": "flow status",
    "flow:start": "flow start",
    "flow:stop": "flow stop",
    "flow:coverage": "flow coverage",
    "flow:all": "flow check --all"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/luigi055/tin-marin.git"
  },
  "keywords": [
    "react",
    "react-router-v4",
    "webpack-v3",
    "boilerplate",
    "Tin Marin"
  ],
  "author": "Pedro La Rosa",
  "license": "MIT",
  "devDependencies": {},
  "dependencies": {
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "enzyme-to-json": "^3.3.1",
    "eslint": "4.15.0",
    "eslint-config-airbnb": "16.1.0",
    "eslint-config-prettier": "2.9.0",
    "eslint-config-react": "^1.1.7",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-flow": "^2.29.1",
    "eslint-plugin-flowtype": "2.41.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-prettier": "2.4.0",
    "eslint-plugin-react": "7.5.1",
    "jest": "^22.1.4",
    "jest-enzyme": "^4.2.0",
    "mocha": "4.1.0",
    "prettier": "1.10.2",
    "stylelint": "8.4.0",
    "stylelint-config-standard": "18.0.0",
    "autoprefixer": "7.2.5",
    "babel-core": "6.26.0",
    "babel-eslint": "8.2.1",
    "babel-loader": "7.1.2",
    "babel-plugin-dynamic-import-webpack": "^1.0.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.26.0",
    "css-loader": "0.28.8",
    "css-mqpacker": "6.0.1",
    "compression": "^1.7.1",
    "express": "4.16.2",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.6",
    "flow-bin": "0.63.1",
    "html-loader": "0.5.4",
    "html-webpack-plugin": "2.30.1",
    "image-webpack-loader": "3.4.2",
    "lodash": "^4.17.5",
    "node-sass": "4.7.2",
    "pixrem": "4.0.1",
    "postcss-loader": "2.0.10",
    "postcss-will-change": "2.0.0",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-flip-move": "^3.0.1",
    "react-router-dom": "4.2.2",
    "raf": "^3.4.0",
    "react-hot-loader": "3.1.3",
    "react-test-renderer": "16.2.0",
    "resolve-url-loader": "2.2.1",
    "sass-loader": "6.0.6",
    "script-loader": "0.7.2",
    "static-loader": "^0.1.8",
    "style-loader": "0.19.1",
    "styled-components": "^3.0.2",
    "url-loader": "0.6.2",
    "webpack": "3.10.0",
    "webpack-dev-middleware": "^2.0.5",
    "webpack-dev-server": "2.10.1",
    "webpack-hot-middleware": "^2.21.0",
    "uuid": "^3.1.0"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./node_modules/jest-enzyme/lib/index.js",
    "testEnvironment": "jest-environment-node",
    "collectCoverageFrom": ["*.js", "*.jsx"],
    "coverageThreshold": {
      "global": {
        "statements": 50,
        "branches": 50,
        "functions": 50,
        "lines": 100
      }
    },
    "snapshotSerializers": ["enzyme-to-json/serializer"],
    "setupFiles": ["raf/polyfill"]
  },
  "engines": {
    "node": "8.9.4",
    "npm": "5.6.0",
    "yarn": "1.3.2"
  }
}
